  text-align: left;
}
#rightcol img.avatar { float: left; }
#rightcol h2 { float: left; }
#rightcol h2 {
  margin-left: 10px;
  line-height: 64px;
  vertical-align: middle;
}
#rightcol a { font-weight: bold; }
#footer {
  color: #eeeeee;
  margin: 0 auto;
  text-align: center;
  clear: both;
  margin-top: 40px;
}
