.shadow {
  -moz-box-shadow: 2px 2px 7px #151515;
  -webkit-box-shadow: 2px 2px 7px #151515;
  box-shadow: 2px 2px 7px #151515;
}
body {
  background: #b3b9c0;
  color: #111111;
  text-decoration: none;
  font: 15px "Droid Serif", "Helvetica Neue", Arial, Helvetica, Geneva, sans-serif;
  width: 100%;
  padding: 0;
  margin: 0;
  text-align: center;
  line-height: 20px;
  min-width: 1000px;
}
.futura { font-family: Futura, "Helvetica Neue", Arial, Helvetica, Geneva, sans-serif; }
a {
  color: #1717e5;
  -webkit-transition: color 0.3s linear;
  -moz-transition: color 0.3s linear;
}
a:hover {
  color: #1717e5;
  -webkit-transition: color 0.3s linear;
  -moz-transition: color 0.3s linear;
}
a:visited { color: #6262ef; }
a.white {
  color: #ffffff;
  -webkit-transition: color 0.3s linear;
  -moz-transition: color 0.3s linear;
}
a:hover.white {
  color: #1717e5;
  -webkit-transition: color 0.3s linear;
  -moz-transition: color 0.3s linear;
}
h1 { font-size: 3em; }
h2 {
  font-size: 2em;
  font-family: Futura, "Helvetica Neue", Arial, Helvetica, Geneva, sans-serif;
}
h3 {
  font-size: 1.6em;
  line-height: 2em;
  font-family: Futura, "Helvetica Neue", Arial, Helvetica, Geneva, sans-serif;
}
p { font-size: 1em; }
.fullpagebar {
  background-color: #272727;
  border-right: #1212b7 solid 10px;
  font-weight: bold;
  color: #ffffff;
  padding-right: 10px;
  line-height: 30px;
}
#login {
  background-color: #272727;
  border-right: #1212b7 solid 10px;
  font-weight: bold;
  color: #ffffff;
  padding-right: 10px;
  line-height: 30px;
  text-align: right;
  min-width: 999px;
}
#login #page a { margin-right: 24px; }
#login #minilogo {
  line-height: 60px;
  background-image: url("../img/minilogo.png");
  width: 204px;
  height: 61px;
  background-repeat: no-repeat;
  background-attachment: top left;
  margin-left: 20px;
}
#login #minilogo a {
  display: block;
  width: 100%;
  height: 100%;
}
.post {
  -webkit-border-top-right-radius: 15px;
  -moz-border-radius-topright: 15px;
  border-top-right-radius: 15px;
  color: #111199;
  -webkit-border-bottom-left-radius: 15px;
  -moz-border-radius-bottomleft: 15px;
  border-bottom-left-radius: 15px;
  -moz-box-shadow: 2px 2px 7px #151515;
  -webkit-box-shadow: 2px 2px 7px #151515;
  box-shadow: 2px 2px 7px #151515;
  border-right: #1212b7 solid 10px;
  color: #eeeeee;
  line-height: 40px;
  padding: 20px;
  margin: 40px;
  width: 60%;
  background-color: #272727;
  text-align: left;
}
.post img.avatar { float: left; }
.post h2 { float: left; }
.post h2 {
  margin-left: 10px;
  line-height: 64px;
  vertical-align: middle;
}
.post a { font-weight: bold; }
#services { line-height: 25px; }
#content {
  width: 600px;
  float: left;
}
#content p { line-height: 30px; }
#rightcol {
  width: 290px;
  float: left;
  margin-left: 40px;
  -webkit-border-top-right-radius: 15px;
  -moz-border-radius-topright: 15px;
  border-top-right-radius: 15px;
